{
    "version": 2,
    "builds": [
      {
        "src": "dist/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/main/index.js"
      }
    ]
  }