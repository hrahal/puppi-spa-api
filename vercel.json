{
    "version": 2,
    "builds": [
      {
        "src": "dist/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/main.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }